{
  "name": "n4m-posenet",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow-models/posenet": "0.2.2",
    "@tensorflow/tfjs": "0.13.0",
    "socket.io": "2.1.1",
